@model ICollection<TodoTaskPartner>

@if (Model != null)
{
    @if (Model.Count > 0)
    {
        @Html.Raw("Làm cùng với:&nbsp")
    }
    @foreach (var item in Model)
    {
        <span class="tasklist-item--partner" data-toggle="tooltip" title="@item.Staff.FullName">
            @item.Staff.FirstName
        </span>
    }
}